package day3;

public class cons {
	String name;
	int year;
	String language;
	 cons(String name, int year,String lang) {
		this.name=name;
		this.language=language;
		this.year=year;
		
		}cons(cons copy){
			
			this.name=copy.name;
			this.language=copy.language;
			this.year=copy.year;
	
		}
	 
		void Display(){
		System.out.println("program"+" " +name+"  "+ year+"  "  );
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("original constractor");
		cons ref=new cons("java",1991,"program");
		ref.Display();
		System.out.println("copy");
		cons ref2=new cons("java",1991,"program");
		ref2.Display();
		
	}

}